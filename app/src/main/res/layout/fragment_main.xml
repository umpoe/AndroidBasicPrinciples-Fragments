<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.u.fragments.Fragments.MainFragment">


    <Button
        android:id="@+id/buttonFrame1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="173dp"
        android:text="Frame1" />

    <Button
        android:id="@+id/buttonFrame2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/buttonFrame1"
        android:layout_alignStart="@+id/buttonFrame1"
        android:layout_below="@+id/buttonFrame1"
        android:layout_marginTop="31dp"
        android:text="Frame2" />
</RelativeLayout>
